---
type Props = {
  framework?: string;
  name?: string;
  version?: string;
  pathname?: string | URL;
};

const {
  name = "x-analytics",
  version = "0.1.0",
  framework = "astro",
  pathname = "",
} = Astro.props;
---

<astro-analytics
  data-sdkn={name + `/${framework}`}
  data-sdkv={version}
  data-pathname={pathname}
  data-prod={`${import.meta.env.PROD}`}
>
</astro-analytics>
