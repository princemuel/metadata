DISCORD_ID=XXXXXXXXXXXXXXXXXXXXXXXXXXXXXX
DISCORD_SECRET=XXXXXXXXXXXXXXXXXXXXXXXXXX
# Google Oauth
GOOGLE_ID=XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX
GOOGLE_SECRET=XXXXXXXXXXXXXXXXXXXXXXXXXXX

RESEND_API_KEY=XXXXXXXXXXXXXXXXXXXXXXXXXXX


REVALIDATE_SECRET=
