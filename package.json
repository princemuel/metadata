{
  "name": "princemuel.com",
  "type": "module",
  "version": "0.3.0",
  "description": "My personal website built with Astro, Qwik, Tailwind CSS, MDX, and the Github API",
  "private": true,
  "repository": "git@github.com:princemuel/princemuel.com.git",
  "author": "princemuel <vansomecsam@gmail.com>",
  "scripts": {
    "dev": "astro dev --host",
    "start": "astro dev",
    "build": "astro build",
    "build:dev": "vercel dev",
    "build:env": "npm run vercel link && npm run vercel env pull",
    "build:prod": "astro check && astro build",
    "build:force": "astro check && astro build -- --force",
    "preview": "astro preview",
    "astro": "astro",
    "typecheck": "tsc --noEmit",
    "fmt:write": "prettier --write .",
    "fmt:check": "prettier --check .",
    "fmt:lint": "npm run typecheck && npm run lint:fix && npm run fmt:write",
    "lint": "npx oxlint -A all --tsconfig ./tsconfig.json --import-plugin --jsx-a11y-plugin --promise-plugin",
    "lint:fix": "npm run lint -- --fix",
    "db:generate": "cargo prisma generate",
    "db:migrate": "cargo prisma migrate dev",
    "db:deploy": "cargo prisma migrate deploy",
    "db:studio": "cargo prisma studio",
    "prepare": "node .husky/install.mjs",
    "lint-staged": "lint-staged",
    "postinstall": "cargo prisma generate"
  },
  "dependencies": {
    "@astrojs/mdx": "^3.1.3",
    "@astrojs/rss": "^4.0.7",
    "@astrojs/tailwind": "^5.1.0",
    "@astrojs/vercel": "^7.7.2",
    "@t3-oss/env-core": "^0.11.0",
    "@upstash/ratelimit": "^2.0.1",
    "@upstash/redis": "^1.34.0",
    "@vercel/functions": "^1.4.0",
    "@vercel/og": "^0.6.2",
    "@vercel/speed-insights": "^1.0.12",
    "astro-expressive-code": "^0.35.6",
    "astro-icon": "^1.1.1",
    "isbot": "^5.1.16",
    "octokit": "^4.0.2",
    "outvariant": "^1.4.3",
    "resend": "^4.0.0",
    "sharp": "^0.33.5",
    "tailwind-variants": "^0.2.1"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.2",
    "@astrojs/markdown-remark": "5.2.0",
    "@astrojs/partytown": "^2.1.1",
    "@astrojs/sitemap": "^3.1.6",
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@expressive-code/plugin-collapsible-sections": "^0.35.6",
    "@expressive-code/plugin-line-numbers": "^0.35.6",
    "@iconify-json/fa": "^1.1.9",
    "@iconify-json/logos": "^1.1.44",
    "@iconify-json/lucide": "^1.1.206",
    "@iconify-json/mdi": "^1.1.68",
    "@iconify-json/ri": "^1.1.22",
    "@kindspells/astro-shield": "^1.3.6",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.14",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/node": "^22.4.0",
    "@types/sanitize-html": "^2.11.0",
    "@vite-pwa/assets-generator": "^0.2.4",
    "@vite-pwa/astro": "^0.4.0",
    "@vite-pwa/workbox-window": "^8.0.0",
    "astro": "^4.14.2",
    "astro-meta-tags": "^0.3.0",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.9",
    "marked": "^14.0.0",
    "mdast-util-to-string": "^4.0.0",
    "oxlint": "^0.7.2",
    "prettier": "^3.3.3",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-organize-imports": "^4.0.0",
    "prettier-plugin-prisma": "^5.0.0",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "reading-time": "^1.5.0",
    "rehype-autolink-headings": "^7.1.0",
    "remark-directive": "^3.0.0",
    "remark-emoji": "^5.0.1",
    "remark-toc": "^9.0.0",
    "schema-dts": "^1.1.2",
    "tailwind-scrollbar": "^3.1.0",
    "tailwindcss": "^3.4.10",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "~5.5.4",
    "unist-util-visit": "^5.0.0",
    "vercel": "^36.0.0",
    "vite-plugin-pwa": "^0.20.1"
  },
  "lint-staged": {
    "**/*.{js,ts,astro}": "oxlint",
    "**/*.{md,mdx,astro}": [
      "astro check"
    ]
  },
  "packageManager": "pnpm@9.7.1+sha512.faf344af2d6ca65c4c5c8c2224ea77a81a5e8859cbc4e06b1511ddce2f0151512431dd19e6aff31f2c6a8f5f2aced9bd2273e1fed7dd4de1868984059d2c4247"
