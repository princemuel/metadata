{
  "name": "princemuel.com",
  "type": "module",
  "version": "0.3.0",
  "description": "My personal website built with Astro, Qwik, Tailwind CSS, MDX, and the Github API",
  "private": true,
  "repository": "git@github.com:princemuel/princemuel.com.git",
  "author": "princemuel <vansomecsam@gmail.com>",
  "license": "MIT",
  "packageManager": "npm@10.8.1+sha512.0e9d42e92bd2318408ed81eaff2da5f78baf23ee7d12a6eed44a6e2901d0f29d7ab715d1b918ade601f72e769a824d9a5c322383f22bbbda5dd396e79de2a077",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "dev": "astro dev --host",
    "open": "astro dev --host --open",
    "functions": "concurrently -k -p \"[{name}]\" -n \"Astro.js,Vercel CLI\" -c \"white.bold,cyan.bold\" \"astro dev\" \"vercel dev\"",
    "start": "astro dev",
    "build": "astro check && astro build",
    "build:dev": "astro check && astro build -- --force",
    "preview": "astro preview",
    "astro": "astro",
    "typecheck": "tsc --noEmit",
    "fmt": "prettier --write .",
    "fmt:check": "prettier --check .",
    "fmt:lint": "oxlint -D correctness -D perf -A debugger -A eqeqeq --jsx-a11y-plugin --fix -c ./.eslintrc.json",
    "lint": "npm run typecheck && npm run fmt:lint && npm run fmt",
    "get:env": "npx vercel link && npx vercel env pull",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "prepare": "node .husky/install.mjs",
    "lint-staged": "lint-staged",
    "postinstall": "prisma generate"
  },
  "prisma": {
    "seed": "node ./prisma/seed.mjs"
  },
  "dependencies": {
    "@astrojs/check": "^0.7.0",
    "@astrojs/mdx": "^3.1.1",
    "@astrojs/partytown": "^2.1.1",
    "@astrojs/rss": "^4.0.6",
    "@astrojs/sitemap": "^3.1.6",
    "@astrojs/tailwind": "^5.1.0",
    "@astrojs/vercel": "^7.7.1",
    "@builder.io/qwik": "^1.5.6",
    "@expressive-code/plugin-collapsible-sections": "^0.35.3",
    "@expressive-code/plugin-line-numbers": "^0.35.3",
    "@fontsource-variable/inconsolata": "^5.0.18",
    "@fontsource-variable/montserrat": "^5.0.19",
    "@fontsource-variable/source-code-pro": "^5.0.19",
    "@fontsource/montserrat-alternates": "^5.0.13",
    "@github/catalyst": "^1.6.0",
    "@prisma/client": "^5.15.1",
    "@qwikdev/astro": "^0.5.15",
    "@t3-oss/env-core": "^0.10.1",
    "@upstash/ratelimit": "^1.2.1",
    "@upstash/redis": "^1.31.5",
    "@vercel/edge": "^1.1.1",
    "@vercel/functions": "^1.0.2",
    "@vercel/og": "^0.6.2",
    "@vercel/speed-insights": "^1.0.12",
    "@vite-pwa/workbox-window": "^8.0.0",
    "astro": "^4.10.3",
    "astro-embed": "^0.7.2",
    "astro-expressive-code": "^0.35.3",
    "astro-font": "^0.0.80",
    "astro-htmx": "^1.0.6",
    "astro-icon": "^1.1.0",
    "htmx.org": "^1.9.12",
    "isbot": "^5.1.9",
    "octokit": "^4.0.2",
    "openai": "^4.51.0",
    "outvariant": "^1.4.2",
    "resend": "^3.3.0",
    "sanitize-html": "^2.13.0",
    "sharp": "^0.33.4",
    "slugify": "^1.6.6",
    "tailwind-variants": "^0.2.1",
    "typescript": "^5.4.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@iconify-json/fa": "^1.1.8",
    "@iconify-json/logos": "^1.1.43",
    "@iconify-json/lucide": "^1.1.193",
    "@iconify-json/mdi": "^1.1.66",
    "@kindspells/astro-shield": "^1.3.6",
    "@tailwindcss/typography": "^0.5.13",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/sanitize-html": "^2.11.0",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "@vite-pwa/assets-generator": "^0.2.4",
    "@vite-pwa/astro": "^0.4.0",
    "astro-eslint-parser": "^1.0.2",
    "concurrently": "^8.2.2",
    "css-has-pseudo": "^6.0.5",
    "eslint": "^8.57.0",
    "eslint-plugin-astro": "^1.2.2",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-oxlint": "^0.4.0",
    "eslint-plugin-qwik": "^1.5.7",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "marked": "^13.0.0",
    "mdast-util-to-string": "^4.0.0",
    "oxlint": "^0.4.4",
    "prettier": "^3.3.2",
    "prettier-plugin-astro": "^0.14.0",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-prisma": "^5.0.0",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "prisma": "^5.15.1",
    "reading-time": "^1.5.0",
    "rehype-autolink-headings": "^7.1.0",
    "remark-directive": "^3.0.0",
    "remark-emoji": "^5.0.0",
    "remark-toc": "^9.0.0",
    "schema-dts": "^1.1.2",
    "tailwind-scrollbar": "^3.1.0",
    "tailwindcss": "^3.4.4",
    "tailwindcss-animate": "^1.0.7",
    "unist-util-visit": "^5.0.0",
    "vercel": "^34.2.7"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,astro}": "oxlint",
    "**/*.{md,mdx,astro}": [
      "astro check"
    ]
  }
}
