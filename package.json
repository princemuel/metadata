{
  "name": "princemuel.com",
  "type": "module",
  "version": "0.3.0",
  "description": "My personal website built with Astro, Qwik, Tailwind CSS, MDX, and the Github API",
  "private": true,
  "repository": "git@github.com:princemuel/princemuel.com.git",
  "author": "princemuel <vansomecsam@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "astro dev --host",
    "start": "astro dev",
    "build": "astro build",
    "build:dev": "vercel dev",
    "build:env": "npm run vercel link && npm run vercel env pull",
    "build:prod": "astro check && astro build",
    "build:force": "astro check && astro build -- --force",
    "preview": "astro preview",
    "astro": "astro",
    "typecheck": "tsc --noEmit",
    "fmt:write": "prettier --write .",
    "fmt:check": "prettier --check .",
    "fmt:lint": "npm run typecheck && npm run lint:fix && npm run fmt:write",
    "lint": "npx oxlint -A all --tsconfig ./tsconfig.json --import-plugin --jsx-a11y-plugin --promise-plugin",
    "lint:fix": "npm run lint -- --fix",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "prepare": "node .husky/install.mjs",
    "lint-staged": "lint-staged",
    "postinstall": "prisma generate"
  },
  "prisma": {
    "seed": "node ./prisma/seed.mjs"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.2",
    "@astrojs/mdx": "^3.1.3",
    "@astrojs/partytown": "^2.1.1",
    "@astrojs/rss": "^4.0.7",
    "@astrojs/sitemap": "^3.1.6",
    "@astrojs/tailwind": "^5.1.0",
    "@astrojs/vercel": "^7.7.2",
    "@builder.io/qwik": "^1.8.0",
    "@expressive-code/plugin-collapsible-sections": "^0.35.6",
    "@expressive-code/plugin-line-numbers": "^0.35.6",
    "@prisma/client": "5.18.0",
    "@qwikdev/astro": "^0.6.3",
    "@t3-oss/env-core": "^0.11.0",
    "@upstash/ratelimit": "^2.0.1",
    "@upstash/redis": "^1.34.0",
    "@vercel/edge": "^1.1.2",
    "@vercel/functions": "^1.4.0",
    "@vercel/og": "^0.6.2",
    "@vercel/speed-insights": "^1.0.12",
    "@vite-pwa/workbox-window": "^8.0.0",
    "astro": "^4.13.3",
    "astro-embed": "^0.7.2",
    "astro-expressive-code": "^0.35.6",
    "astro-htmx": "^1.0.6",
    "astro-icon": "^1.1.0",
    "astro-meta-tags": "^0.3.0",
    "htmx.org": "^2.0.1",
    "isbot": "^5.1.14",
    "octokit": "^4.0.2",
    "outvariant": "^1.4.3",
    "resend": "^3.5.0",
    "sanitize-html": "^2.13.0",
    "tailwind-variants": "^0.2.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@astrojs/markdown-remark": "^5.2.0",
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@iconify-json/fa": "^1.1.9",
    "@iconify-json/logos": "^1.1.44",
    "@iconify-json/lucide": "^1.1.205",
    "@iconify-json/mdi": "^1.1.68",
    "@iconify-json/ri": "^1.1.22",
    "@kindspells/astro-shield": "^1.3.6",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.14",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/sanitize-html": "^2.11.0",
    "@vite-pwa/assets-generator": "^0.2.4",
    "@vite-pwa/astro": "^0.4.0",
    "css-has-pseudo": "^7.0.0",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.8",
    "markdownlint-cli": "^0.41.0",
    "marked": "^14.0.0",
    "mdast-util-to-string": "^4.0.0",
    "oxlint": "^0.7.0",
    "prettier": "^3.3.3",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-organize-imports": "^4.0.0",
    "prettier-plugin-prisma": "^5.0.0",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "prisma": "5.18.0",
    "reading-time": "^1.5.0",
    "rehype-autolink-headings": "^7.1.0",
    "remark-directive": "^3.0.0",
    "remark-emoji": "^5.0.1",
    "remark-toc": "^9.0.0",
    "schema-dts": "^1.1.2",
    "tailwind-scrollbar": "^3.1.0",
    "tailwindcss": "^3.4.9",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.0.1",
    "unist-util-visit": "^5.0.0",
    "vercel": "^35.2.4",
    "vite": "^5.4.0",
    "vite-plugin-pwa": "^0.20.1"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,astro}": "oxlint",
    "**/*.{md,mdx,astro}": [
      "astro check"
    ]
  },
  "packageManager": "pnpm@9.7.0"
}
