{
  "name": "webapp",
  "type": "module",
  "version": "0.2.0",
  "description": "My personal portfolio website built with Astro, Tailwind CSS, MDX and the Github API",
  "private": true,
  "repository": "git@github.com:princemuel/webapp.git",
  "author": "princemuel <vansomecsam@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "astro dev --host",
    "start": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "type-check": "tsc --noEmit",
    "format:write": "prettier ./src --write \"**/*.{js,mjs,jsx,ts,tsx,md,mdx,html,css}\" --ignore-unknown --cache",
    "format:check": "prettier ./src --check \"**/*.{js,mjs,jsx,ts,tsx,md,mdx,html,css}\" --ignore-unknown --cache",
    "get:env": "npx vercel link && npx vercel env pull",
    "prepare": "husky install"
  },
  "dependencies": {
    "@astrojs/check": "^0.3.4",
    "@astrojs/mdx": "^2.0.3",
    "@astrojs/react": "^3.0.9",
    "@astrojs/rss": "^4.0.1",
    "@astrojs/sitemap": "^3.0.4",
    "@astrojs/tailwind": "^5.1.0",
    "@astrojs/vercel": "^6.1.3",
    "@conform-to/zod": "^0.9.1",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@vercel/og": "^0.6.2",
    "@vercel/speed-insights": "^1.0.2",
    "astro": "^4.1.1",
    "astro-icon": "^1.0.2",
    "framer-motion": "^10.17.9",
    "mdast-util-to-string": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.12.0",
    "reading-time": "^1.5.0",
    "rehype-pretty-code": "^0.12.3",
    "resend": "^2.1.0",
    "tailwind-variants": "^0.1.20",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.10",
    "@total-typescript/ts-reset": "^0.5.1",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@typescript-eslint/parser": "^6.18.0",
    "astro-eslint-parser": "^0.16.0",
    "eslint": "^8.56.0",
    "eslint-plugin-astro": "^0.31.0",
    "eslint-plugin-react": "^7.33.2",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.1",
    "prettier-plugin-astro": "^0.12.3",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "rehype-shikiji": "^0.9.17",
    "shikiji-transformers": "^0.9.17",
    "tailwindcss-animate": "^1.0.7"
  },
  "lint-staged": {
    "*": "npm run format:write --"
  }
}
