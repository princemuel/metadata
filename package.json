{
  "name": "princemuel.com",
  "type": "module",
  "version": "0.2.0",
  "description": "My personal portfolio website built with Astro, Tailwind CSS, MDX and the Github API",
  "private": true,
  "repository": "git@github.com:princemuel/webapp.git",
  "author": "princemuel <vansomecsam@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "astro dev --host",
    "start": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "typecheck": "tsc --noEmit",
    "format:write": "prettier ./src --write \"**/*.{js,mjs,jsx,ts,tsx,md,mdx,astro,html,css}\" --ignore-unknown --cache",
    "format:check": "prettier ./src --check \"**/*.{js,mjs,jsx,ts,tsx,md,mdx,astro,html,css}\" --ignore-unknown --cache",
    "format:lint": "oxlint --deny-warnings",
    "lint": "npm run typecheck && npm run format:lint && npm run format:write",
    "get:env": "npx vercel link && npx vercel env pull",
    "prepare": "husky install",
    "lint-staged": "lint-staged"
  },
  "dependencies": {
    "@astrojs/alpinejs": "^0.4.0",
    "@astrojs/check": "^0.5.4",
    "@astrojs/mdx": "^2.1.1",
    "@astrojs/react": "^3.0.10",
    "@astrojs/rss": "^4.0.5",
    "@astrojs/sitemap": "^3.0.5",
    "@astrojs/tailwind": "^5.1.0",
    "@astrojs/vercel": "^7.3.1",
    "@builder.io/qwik": "^1.4.5",
    "@builder.io/qwik-city": "^1.4.5",
    "@conform-to/react": "^1.0.1",
    "@conform-to/zod": "^1.0.1",
    "@expressive-code/plugin-collapsible-sections": "^0.32.4",
    "@google-cloud/local-auth": "^3.0.1",
    "@headlessui/react": "^1.7.18",
    "@qwikdev/astro": "^0.5.2",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@types/alpinejs": "^3.13.6",
    "@vercel/og": "^0.6.2",
    "alpinejs": "^3.13.5",
    "astro": "^4.4.0",
    "astro-expressive-code": "^0.32.4",
    "astro-font": "^0.0.77",
    "astro-htmx": "^1.0.5",
    "astro-icon": "^1.1.0",
    "framer-motion": "^11.0.5",
    "google-auth-library": "^9.6.3",
    "googleapis": "^133.0.0",
    "htmx.org": "^1.9.10",
    "react-icons": "^5.0.1",
    "resend": "^3.2.0",
    "tailwind-variants": "^0.2.0",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.2",
    "@tailwindcss/typography": "^0.5.10",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "@vite-pwa/astro": "^0.3.0",
    "astro-eslint-parser": "^0.16.3",
    "conventional-changelog-atom": "^4.0.0",
    "css-has-pseudo": "^6.0.1",
    "eslint": "^8.56.0",
    "eslint-plugin-astro": "^0.31.4",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-qwik": "^1.4.5",
    "eslint-plugin-react": "^7.33.2",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "mdast-util-to-string": "^4.0.0",
    "oxlint": "^0.2.9",
    "postcss-preset-env": "^9.3.0",
    "prettier": "^3.2.5",
    "prettier-plugin-astro": "^0.13.0",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "reading-time": "^1.5.0",
    "rehype-accessible-emojis": "^0.3.2",
    "rehype-autolink-headings": "^7.1.0",
    "remark-toc": "^9.0.0",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "unist-util-visit": "^5.0.0"
  },
  "lint-staged": {
    "**/*.{js,mjs,cjs,jsx,ts,mts,cts,tsx,vue,astro,svelte}": "oxlint",
    "*.astro": [
      "astro check",
      "oxlint"
    ],
    "*.md": "astro check"
  }
}
