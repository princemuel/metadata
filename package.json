{
  "name": "princemuel.com",
  "type": "module",
  "version": "0.3.0",
  "description": "My personal portfolio website built with Astro, Tailwind CSS, MDX and the Github API",
  "private": true,
  "repository": "git@github.com:princemuel/webapp.git",
  "author": "princemuel <vansomecsam@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "astro dev --open --host",
    "start": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "clean": "rimraf ./.astro ./dist ./vercel",
    "typecheck": "tsc --noEmit",
    "fmt": "prettier --write .",
    "fmt:check": "prettier --check .",
    "fmt:lint": "oxlint -D correctness -D perf -A debugger -A eqeqeq --import-plugin --jsx-a11y-plugin --fix -c ./.eslintrc.json",
    "lint": "npm run typecheck && npm run fmt:lint && npm run fmt",
    "get:env": "npx vercel link && npx vercel env pull",
    "prepare": "node .husky/install.mjs",
    "lint-staged": "lint-staged"
  },
  "dependencies": {
    "@alpinejs/intersect": "^3.13.8",
    "@alpinejs/persist": "^3.13.8",
    "@astrojs/alpinejs": "^0.4.0",
    "@astrojs/check": "^0.5.10",
    "@astrojs/db": "^0.10.4",
    "@astrojs/mdx": "^2.3.1",
    "@astrojs/rss": "^4.0.5",
    "@astrojs/sitemap": "^3.1.3",
    "@astrojs/tailwind": "^5.1.0",
    "@astrojs/vercel": "^7.5.3",
    "@builder.io/qwik": "^1.5.2",
    "@expressive-code/plugin-collapsible-sections": "^0.35.0",
    "@expressive-code/plugin-line-numbers": "^0.35.0",
    "@modular-forms/qwik": "^0.23.1",
    "@qwikdev/astro": "^0.5.14",
    "@qwikest/icons": "^0.0.13",
    "@t3-oss/env-core": "^0.9.2",
    "@vercel/og": "^0.6.2",
    "@vercel/speed-insights": "^1.0.10",
    "alpinejs": "^3.13.8",
    "astro": "^4.6.2",
    "astro-expressive-code": "^0.35.0",
    "astro-font": "^0.0.80",
    "astro-icon": "^1.1.0",
    "astro-og-canvas": "^0.4.2",
    "octokit": "^3.2.0",
    "outvariant": "^1.4.2",
    "resend": "^3.2.0",
    "sanitize-html": "^2.13.0",
    "sharp": "^0.33.3",
    "simple-stack-form": "^0.1.12",
    "simple-stack-stream": "^0.3.3",
    "tailwind-variants": "^0.2.1",
    "typescript": "^5.4.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.2",
    "@commitlint/config-conventional": "^19.2.2",
    "@iconify-json/lucide": "^1.1.182",
    "@tailwindcss/typography": "^0.5.12",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/alpinejs": "^3.13.10",
    "@types/sanitize-html": "^2.11.0",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "astro-eslint-parser": "^0.17.0",
    "css-has-pseudo": "^6.0.3",
    "eslint": "^8.57.0",
    "eslint-plugin-astro": "^0.34.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-oxlint": "^0.2.9",
    "eslint-plugin-qwik": "^1.5.2",
    "globals": "^15.0.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "mdast-util-to-string": "^4.0.0",
    "oxlint": "^0.2.17",
    "postcss-import": "^16.1.0",
    "postcss-preset-env": "^9.5.5",
    "prettier": "^3.2.5",
    "prettier-plugin-astro": "^0.13.0",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "reading-time": "^1.5.0",
    "rehype-accessible-emojis": "^0.3.2",
    "rehype-autolink-headings": "^7.1.0",
    "remark-toc": "^9.0.0",
    "tailwindcss": "^3.4.3",
    "tailwindcss-animate": "^1.0.7",
    "unist-util-visit": "^5.0.0"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,astro}": "oxlint",
    "**/*.{md,astro}": [
      "astro check"
    ]
  }
}
