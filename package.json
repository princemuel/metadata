{
  "name": "princemuel.com",
  "type": "module",
  "version": "0.3.0",
  "description": "My personal website built with Astro, Qwik, Tailwind CSS, MDX, and the Github API",
  "private": true,
  "repository": "git@github.com:princemuel/princemuel.com.git",
  "author": "princemuel <vansomecsam@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": "^18.18.0 || >=20.0.0"
  },
  "scripts": {
    "dev": "astro dev --host",
    "open": "astro dev --host --open",
    "functions": "concurrently -k -p \"[{name}]\" -n \"Astro.js,Vercel CLI\" -c \"white.bold,cyan.bold\" \"astro dev\" \"vercel dev\"",
    "start": "astro dev",
    "build": "astro check && astro build",
    "build:dev": "vercel dev",
    "build:force": "astro check && astro build -- --force",
    "preview": "astro preview",
    "astro": "astro",
    "typecheck": "tsc --noEmit",
    "fmt:write": "prettier --write .",
    "fmt:check": "prettier --check .",
    "fmt:lint": "npm run typecheck && npm run lint:fix && npm run fmt:write",
    "lint": "npx oxlint && npx eslint",
    "lint:fix": "npm run lint -- --fix",
    "get:env": "npm run vercel link && npm run vercel env pull",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "prepare": "node .husky/install.mjs",
    "lint-staged": "lint-staged",
    "postinstall": "prisma generate"
  },
  "prisma": {
    "seed": "node ./prisma/seed.mjs"
  },
  "dependencies": {
    "@astrojs/check": "^0.8.2",
    "@astrojs/mdx": "^3.1.2",
    "@astrojs/partytown": "^2.1.1",
    "@astrojs/rss": "^4.0.7",
    "@astrojs/sitemap": "^3.1.6",
    "@astrojs/tailwind": "^5.1.0",
    "@astrojs/vercel": "^7.7.2",
    "@builder.io/qwik": "^1.7.1",
    "@expressive-code/plugin-collapsible-sections": "^0.35.3",
    "@expressive-code/plugin-line-numbers": "^0.35.3",
    "@fontsource-variable/figtree": "^5.0.21",
    "@fontsource-variable/inconsolata": "^5.0.18",
    "@fontsource-variable/jetbrains-mono": "^5.0.21",
    "@fontsource-variable/karla": "^5.0.21",
    "@fontsource-variable/montserrat": "^5.0.19",
    "@fontsource-variable/rubik": "^5.0.22",
    "@fontsource-variable/source-code-pro": "^5.0.19",
    "@fontsource/montserrat-alternates": "^5.0.13",
    "@prisma/client": "5.17.0",
    "@qwikdev/astro": "^0.6.1",
    "@t3-oss/env-core": "^0.10.1",
    "@upstash/ratelimit": "^2.0.0",
    "@upstash/redis": "^1.33.0",
    "@vercel/edge": "^1.1.1",
    "@vercel/functions": "^1.4.0",
    "@vercel/og": "^0.6.2",
    "@vercel/speed-insights": "^1.0.12",
    "@vite-pwa/workbox-window": "^8.0.0",
    "astro": "^4.11.6",
    "astro-embed": "^0.7.2",
    "astro-expressive-code": "^0.35.3",
    "astro-font": "^0.0.81",
    "astro-htmx": "^1.0.6",
    "astro-icon": "^1.1.0",
    "astro-meta-tags": "^0.3.0",
    "htmx.org": "^2.0.1",
    "isbot": "^5.1.13",
    "octokit": "^4.0.2",
    "openai": "^4.52.7",
    "outvariant": "^1.4.3",
    "resend": "^3.4.0",
    "sanitize-html": "^2.13.0",
    "sharp": "^0.33.4",
    "tailwind-variants": "^0.2.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@astrojs/markdown-remark": "^5.1.1",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/compat": "^1.1.1",
    "@eslint/js": "^9.7.0",
    "@iconify-json/fa": "^1.1.8",
    "@iconify-json/logos": "^1.1.43",
    "@iconify-json/lucide": "^1.1.200",
    "@iconify-json/mdi": "^1.1.67",
    "@iconify-json/ri": "^1.1.21",
    "@kindspells/astro-shield": "^1.3.6",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.13",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/eslint__js": "^8.42.3",
    "@types/sanitize-html": "^2.11.0",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "@vite-pwa/assets-generator": "^0.2.4",
    "@vite-pwa/astro": "^0.4.0",
    "concurrently": "^8.2.2",
    "css-has-pseudo": "^6.0.5",
    "eslint": "^9.7.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-astro": "^1.2.3",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-oxlint": "^0.5.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-qwik": "^1.7.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unicorn": "^54.0.0",
    "husky": "^9.1.1",
    "lint-staged": "^15.2.7",
    "markdownlint-cli": "^0.41.0",
    "marked": "^13.0.2",
    "mdast-util-to-string": "^4.0.0",
    "oxlint": "^0.6.1",
    "prettier": "^3.3.3",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-organize-imports": "^4.0.0",
    "prettier-plugin-prisma": "^5.0.0",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "prisma": "5.17.0",
    "reading-time": "^1.5.0",
    "rehype-autolink-headings": "^7.1.0",
    "remark-directive": "^3.0.0",
    "remark-emoji": "^5.0.1",
    "remark-toc": "^9.0.0",
    "schema-dts": "^1.1.2",
    "tailwind-scrollbar": "^3.1.0",
    "tailwindcss": "^3.4.6",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.16.1",
    "unist-util-visit": "^5.0.0",
    "vercel": "^35.0.3"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,astro}": "oxlint",
    "**/*.{md,mdx,astro}": [
      "astro check"
    ]
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
