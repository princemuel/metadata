{
  "name": "princemuel.com",
  "type": "module",
  "version": "0.3.0",
  "description": "My personal website built with Astro, Qwik, Tailwind CSS, MDX, and the Github API",
  "private": true,
  "repository": "git@github.com:princemuel/princemuel.com.git",
  "author": "princemuel <vansomecsam@gmail.com>",
  "scripts": {
    "dev": "astro dev --remote --host",
    "start": "astro dev --remote",
    "build": "astro check && astro build --remote",
    "vercel:dev": "vercel dev",
    "vercel:env": "npm run vercel link && npm run vercel env pull",
    "build:prod": "astro check && astro build --remote",
    "build:force": "astro check && astro build --remote -- --force",
    "preview": "astro preview",
    "astro": "astro",
    "typecheck": "tsc --noEmit",
    "db:gen": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "postinstall": "prisma generate"
