{
  "name": "princemuel",
  "type": "module",
  "version": "0.2.0",
  "description": "My personal portfolio website built with Astro, Tailwind CSS, MDX and the Github API",
  "private": true,
  "repository": "git@github.com:princemuel/princemuel.git",
  "author": "princemuel <vansomecsam@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "astro dev --host",
    "start": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "type-check": "tsc --noEmit",
    "format:write": "prettier ./src --write \"**/*.{js,mjs,jsx,ts,tsx,md,mdx,html,css}\" --ignore-unknown --cache",
    "format:check": "prettier ./src --check \"**/*.{js,mjs,jsx,ts,tsx,md,mdx,html,css}\" --ignore-unknown --cache",
    "get:env": "npx vercel link && npx vercel env pull",
    "prepare": "husky install"
  },
  "dependencies": {
    "@astrojs/check": "^0.3.4",
    "@astrojs/mdx": "^2.0.2",
    "@astrojs/react": "^3.0.8",
    "@astrojs/rss": "^4.0.1",
    "@astrojs/sitemap": "^3.0.3",
    "@astrojs/tailwind": "^5.0.4",
    "@astrojs/vercel": "^6.1.0",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@vercel/og": "^0.6.1",
    "astro": "^4.0.7",
    "astro-icon": "^1.0.0",
    "framer-motion": "^10.16.16",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.12.0",
    "resend": "^2.1.0",
    "tailwind-variants": "^0.1.19",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.10",
    "@total-typescript/ts-reset": "^0.5.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.1",
    "prettier-plugin-astro": "^0.12.2",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "tailwindcss-animate": "^1.0.7"
  },
  "lint-staged": {
    "*": "npm run format:write --"
  }
}
