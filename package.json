{
  "name": "princemuel",
  "type": "module",
  "version": "0.2.0",
  "description": "My personal portfolio website built with Astro, Tailwind CSS, MDX and the Github API",
  "private": true,
  "repository": "git@github.com:princemuel/princemuel.git",
  "author": "princemuel <vansomecsam@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "type-check": "tsc --noEmit",
    "format:write": "prettier ./src --write \"**/*.{js,mjs,jsx,ts,tsx,md,mdx,html,css}\" --cache",
    "format:check": "prettier ./src --check \"**/*.{js,mjs,jsx,ts,tsx,md,mdx,html,css}\" --cache",
    "get:env": "npx vercel link && npx vercel env pull",
    "prepare": "husky install"
  },
  "dependencies": {
    "@astrojs/check": "^0.3.1",
    "@astrojs/mdx": "^2.0.0",
    "@astrojs/react": "^3.0.7",
    "@astrojs/rss": "^4.0.0",
    "@astrojs/sitemap": "^3.0.3",
    "@astrojs/tailwind": "^5.0.3",
    "@astrojs/vercel": "^6.0.0",
    "@types/react": "^18.2.42",
    "@types/react-dom": "^18.2.17",
    "astro": "^4.0.2",
    "astro-icon": "^0.8.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-variants": "^0.1.18",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.3.2"
  },
  "devDependencies": {
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.0",
    "prettier-plugin-astro": "^0.12.2",
    "prettier-plugin-tailwindcss": "^0.5.9"
  },
  "lint-staged": {
    "**/*": "npm run format:write --"
  }
}
